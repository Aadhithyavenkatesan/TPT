<h3>Vanakaam da maapulae!</h3>

<p><a href="/adduser" class="addBtn">Add User</a></p>

<table>
    <thead>
        <tr>
            <td>#</td>
            <td>Name</td>
            <td>Age</td>
            <td>City</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>

    </thead>
    <tbody>
        
    {{#each rows}}
        <tr>
            <td>{{this.ID}}</td>
            <td>{{this.NAME}}</td>
            <td>{{this.AGE}}</td>
            <td>{{this.CITY}}</td>
            <td><a href="/edituser" class="edit">Edit</a></td>
            <td><a href="/deleteuser" class="delete">Delete</a></td>
        </tr>
    {{/each}}

    </tbody>
</table>